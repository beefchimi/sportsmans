!!!
%html#root{:lang => "en"}

	= Haml::Engine.new(File.read("dev/haml/partials/_html_head.haml")).render

	-# 'view_quantity-form' applied where number inputs are needed
	%body#cart_checkout.page_cart-checkout.view_quantity-form

		:plain
			<!-- inject:svg --><!-- endinject -->

		%header#header_common.header_common{"data-scrolled" => "no", "data-cart" => "yes"}
			= Haml::Engine.new(File.read("dev/haml/partials/_header_row-1.haml")).render
			= Haml::Engine.new(File.read("dev/haml/partials/_header_row-2.haml")).render

		%main{:role => "main", "data-container" => "width-926"}
			= Haml::Engine.new(File.read("dev/haml/partials/_cart_table.haml")).render

			#cart_empty-message.cart_empty-message
				%h4 Empty Cart

			= Haml::Engine.new(File.read("dev/haml/partials/_modal_cart-remove.haml")).render

		= Haml::Engine.new(File.read("dev/haml/partials/_html_footer.haml")).render